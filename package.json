{
  "name": "ifman",
  "version": "1.0.0",
  "description": "facility management company",
  "main": "app.js",
  "scripts": {
    "start": "node ./src/bin/www",
    "devstart": "set DEBUG=dev && babel-watch ./src/bin/dev",
    "build": "rm -rf dist && mkdir dist && babel src -s -d dist",
    "test": " set NODE_ENV=test&& npm run reset-db&& nyc mocha -r @babel/register --recursive src/tests/ --exit -timeout 10000",
    "migrate": "sequelize db:migrate",
    "drop-db": "sequelize db:migrate:undo:all",
    "seeders": "sequelize db:seed:all",
    "reset-db": "npm run drop-db&& npm run migrate&& npm run seeders",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls",
    "generate-report": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "lint": "./node_modules/.bin/eslint ./server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/misanj/Ifman.git"
  },
  "keywords": [
    "ifma",
    "node",
    "express",
    "endpoints"
  ],
  "author": "temisan otokuefor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/misanj/Ifman/issues"
  },
  "homepage": "https://github.com/misanj/Ifman#readme",
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@hapi/joi": "^15.1.0",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "babel-watch": "^7.0.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1"
  }
}
